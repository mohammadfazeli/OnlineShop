@model IEnumerable<OnlineShop.ViewModels.Area.Base.Products.ProdcutListDto>

<div class="row">
    <div class="title-section mb-5 col-12">
        <h2 class="text-uppercase p-2">محصولات محبوب</h2>
    </div>
</div>
<div class="row">
    @{
        foreach (var item in Model)
        {
            <div class="col-sm-6 col-md-6 col-xl-3 col-lg-4  item-entry mb-4 ">
                <partial name="_DisplayItems" model="item" />
            </div>
        }
    }
</div>