@model IEnumerable<OnlineShop.ViewModels.Area.Base.Colors.ColorstDto>
@using OnlineShop.Common.Utilities

<div class="product_color_switch">
    @{
        for(int i = 0;i < Model.Count();i++)
        {
            <span class="@( i==0?  "active" : "" )" data-color="@(Model.ElementAt(i).UserCode)"></span>

        }
    }
</div>